{
	"name": "human",
	"type": "dark",
	"colors": {
		"editor.background": "#00003C",
		"editor.foreground": "#FAF8FF",
		"editor.lineHighlightBackground": "#1A2150",

		"selection.background": "#1A2150",
		"focusBorder": "#334064",
		"widget.shadow": "#000000a5",
		"descriptionForeground": "#BFA440",
		"errorForeground": "#A14436",

		"sideBar.background": "#00004d",
		"sideBar.foreground": "#FAF8FF",
		"sideBar.dropBackground": "#202260",
		"sideBar.border": "#4c2b53",
		"sideBarTitle.foreground": "#E7ACB3",
		"sideBarSectionHeader.background": "#4c2b53",

		"activityBar.background": "#1A2150",
		"activityBar.foreground": "#E7ACB3",
		"activityBarBadge.background": "#BFB1FC",
		"activityBarBadge.foreground": "#00003C",
		"activityBar.border": "#4c2b53",

		"button.background": "#C7878E",
		"button.hoverBackground": "#AF6C73",
		"button.foreground": "#01014C",

		"editorGroupHeader.tabsBackground": "#4c2b53",
		"tab.activeBackground": "#2e1a50",
		"tab.activeBorder": "#FAAE50",
		"tab.inactiveBackground": "#5a3354",
		"tab.inactiveForeground": "#EFDDF4",
		"tab.unfocusedActiveForeground": "#E7ACB3",
		"tab.unfocusedInactiveForeground": "#E7ACB3",
		"tab.hoverBackground": "#3d2252",
		"tab.border": "#693b56",

		"titleBar.activeBackground": "#4c2b53",
		"titleBar.activeForeground": "#C7878E",

		"statusBar.background": "#01014C",
		"statusBar.foreground": "#FAAE50",

		"list.activeSelectionBackground": "#43345F",
		"list.focusBackground": "#43345F",
		"list.hoverBackground": "#96545A",
		"list.inactiveSelectionBackground": "#6B3E72",
		"list.highlightForeground": "#FAAE50",
		"list.dropBackground": "#43345F",

		"input.background": "#231f47",
		"input.border": "#4c2b53",
		"input.foreground": "#F5ECFF",

		"settings.dropdownBackground": "#231f47",
		"settings.checkboxBackground": "#231f47",
		"settings.dropdownForeground": "#F5ECFF",
		"settings.checkboxForeground": "#F5ECFF",
		"settings.dropdownBorder": "#4c2b53",
		"settings.checkboxBorder": "#4c2b53",
		"settings.dropdownListBorder": "#4c2b53"
	},
	"tokenColors": [
		{
			"name": "Normal",
			"scope": ["punctuation", "meta.delimiter.comma", "text.html"],
			"settings": {
				"foreground": "#E0F5E7"
			}
		},
		{
			"name": "Variable",
			"scope": ["variable", "entity.name.class"],
			"settings": {
				"foreground": "#fff14e"
			}
		},
		{
			"name": "Prototype Chain",
			"scope": ["variable.language.prototype"],
			"settings": {
				"foreground": "#edc224",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Property",
			"scope": ["variable.other.property", "variable.other"],
			"settings": {
				"foreground": "#ffe894",
				"fontStyle": "normal"
			}
		},
		{
			"name": "Builtin Class",
			"scope": [
				"support.class.builtin",
				"keyword.operator",
				"support.type.object.process"
			],
			"settings": {
				"foreground": "#7fd2fa",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"storage",
				"keyword.control",
				"constant.language.boolean",
				"constant.language.null",
				"variable.language.this",
				"support.type.object.console"
			],
			"settings": {
				"foreground": "#7fd2fa",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Operator",
			"scope": ["keyword.operator.comparison"],
			"settings": {
				"foreground": "#DDE0F4",
				"fontStyle": "normal"
			}
		},
		{
			"name": "ModulePath",
			"scope": ["string.quoted.module.js"],
			"settings": {
				"foreground": "#ff86a2"
			}
		},
		{
			"name": "Function",
			"scope": [
				"entity.name.function.js",
				"support.function.misc",
				"meta.function.method"
			],
			"settings": {
				"foreground": "#4ef892",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Function Parameter",
			"scope": ["meta.function.parameters"],
			"settings": {
				"foreground": "#f0b1f8",
				"fontStyle": "normal"
			}
		},
		{
			"name": "Object Key",
			"scope": [
				"constant.other.object.key",
				"variable.other.readwrite.shorthandpropertyname",
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#95fffa",
				"fontStyle": "normal"
			}
		},
		{
			"name": "Object Value",
			"scope": [
				"support.constant.property-value",
				"meta.property-value variable",
				"meta.tag.block.any string"
			],
			"settings": {
				"foreground": "#fff14e",
				"fontStyle": "normal"
			}
		},
		{
			"name": "Object Method",
			"scope": ["entity.name.function", "meta.function.json"],
			"settings": {
				"foreground": "#95fffa",
				"fontStyle": "bold"
			}
		},
		{
			"name": "String",
			"scope": [
				"string.quoted.single",
				"meta.structure.dictionary.value",
				"string.quoted"
			],
			"settings": {
				"foreground": "#ff6262",
				"fontStyle": "normal"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"punctuation.definition.tag",
				"entity.name.tag",
				"punctuation.definition.entity",
				"entity.other.attribute-name.class",
				"entity.name.tag.reference",
				"meta.property-list variable"
			],
			"settings": {
				"foreground": "#d9a7ff"
			}
		},
		{
			"name": "Tag Attribute",
			"scope": ["entity.other.attribute-name"],
			"settings": {
				"foreground": "#95fffa"
			}
		},
		{
			"name": "Tag Attribute Value",
			"scope": ["meta.tag.inline.any"],
			"settings": {
				"foreground": "#fff14e"
			}
		},
		{
			"name": "Constant Value",
			"scope": [
				"support.constant.color",
				"keyword.other.unit",
				"constant.numeric"
			],
			"settings": {
				"foreground": "#fff14e",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Comment",
			"scope": ["comment", "punctuation.definition.comment"],
			"settings": {
				"foreground": "#887088"
			}
		}
	]
}
